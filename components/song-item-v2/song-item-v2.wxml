<!--components/song-item-v2/song-item-v2.wxml-->
<view class="item" bindtap="onSongItemTap">
  <view class="index">{{index}}</view>
  <view class="info">
    <view class="name">{{itemData.name}}</view>
    <view class="source">
      <image class="icon" src="/assets/images/icons/sq_icon.png"></image>
      <text class="text">{{itemData.ar[0].name}} - {{itemData.al.name}}</text>
    </view>
    <van-icon class="play" name="play-circle-o" />
  </view>
</view>
